<h1>edit location</h1>

<%= form_for (@location) do |f| %>

  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/><br/>

  <%= f.label :description %><br/>
  <%= f.text_area :description %><br/><br/>

  <%= f.label :address %><br/>
  <%= f.text_field :address %><br/><br/>

  <%= f.submit %>
<% end %>
